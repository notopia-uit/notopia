{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "packages/notopia-note-api",
  "projectType": "library",
  "targets": {
    "gen": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec openapi-typescript ../../api/note/dist/openapi.json --output src/index.ts",
        "cwd": "{projectRoot}"
      },
      "dependsOn": [
        {
          "projects": "api/note",
          "target": "bundle:json"
        }
      ]
    }
  }
}
