{
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "name": "page",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["mkdir -p dist", "cp -r ../docs/.vitepress/dist/. dist/"],
        "cwd": "{projectRoot}"
      },
      "outputs": ["dist"],
      "dependsOn": [
        {
          "projects": ["docs"],
          "target": "build"
        }
      ]
    }
  }
}
