{
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "name": "api",
  "projectType": "application",
  "targets": {
    "build:yaml": {
      "dependsOn": ["^build:yaml"]
    },
    "build:json": {
      "dependsOn": ["^build:json"]
    },
    "build": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "executor": "nx:noop",
      "dependsOn": ["^lint"]
    },
    "mock": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node ./tools/mock-api-server.js"
      }
    }
  },
  "implicitDependencies": ["api/note"]
}
